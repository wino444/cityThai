-- Main HUB script (run this in executor)
local UiLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/wino444/winhub/main/ui.lua"))()

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

player.CharacterAdded:Connect(function(newCharacter)
    character = newCharacter
    humanoid = newCharacter:WaitForChild("Humanoid")
end)

local moneyLoop = false
local loopSpeed = 0.1
local autoEat = false
local respawnAtDeathPos = false

UiLibrary:new({
    Name = "TOUx1 HUB",
    Section1 = {
        Image1 = "rbxthumb://type=AvatarHeadShot&id="..player.UserId.."&w=150&h=150",
        Label1 = {"ชื่อ: "..player.Name, 18, 30},
        Label2 = {"จำนวนวันที่สร้าง: "..player.AccountAge.." วัน", 18, 30},
        Label3 = {"คนสร้าง @TOUx1 สร้าง GUI , script \nคนช่วยสร้าง @wino444 สร้าง script หยิบของ", 16, 50}
    },
    Section2 = {
        LabelSpeed = {"ความเร็ว: "..humanoid.WalkSpeed, 18, 30},
        BoxSpeed = {"ความเร็ว: ", function(str)
            local newSpeed = tonumber(str)
            if newSpeed and newSpeed > 0 and newSpeed <= 500 then
                humanoid.WalkSpeed = newSpeed
            end
        end, tostring(humanoid.WalkSpeed)},
        LabelJump = {"กระโดด: "..humanoid.JumpPower, 18, 30},
        BoxJump = {"กระโดด: ", function(str)
            local newJump = tonumber(str)
            if newJump and newJump > 0 and newJump <= 500 then
                humanoid.JumpPower = newJump
            end
        end, tostring(humanoid.JumpPower)},
        ToggleRespawn = {"เกิดกลับที่", respawnAtDeathPos, 'respawnFlag', function(bool)
            respawnAtDeathPos = bool
            if bool then
                if _G.ReviveBackConnection then _G.ReviveBackConnection:Disconnect() end
                _G.LastDeathPos = nil
                _G.ReviveBackConnection = player.CharacterAdded:Connect(function(char)
                    task.wait(0.5)
                    local hrp = char:WaitForChild("HumanoidRootPart")
                    if _G.LastDeathPos then hrp.CFrame = _G.LastDeathPos end
                end)
                task.spawn(function()
                    while player.Character do
                        local char = player.Character
                        local humanoid = char:FindFirstChildOfClass("Humanoid")
                        if humanoid and humanoid.Health <= 0 then
                            local hrp = char:FindFirstChild("HumanoidRootPart")
                            if hrp then _G.LastDeathPos = hrp.CFrame end
                        end
                        task.wait(0.2)
                    end
                end)
            else
                if _G.ReviveBackConnection then _G.ReviveBackConnection:Disconnect() _G.ReviveBackConnection = nil end
                _G.LastDeathPos = nil
            end
        end}
    },
    Section3 = {
        LabelPump = {"ปั้มเงิน", 18, 30},
        TogglePump = {"ปั้มเงิน", moneyLoop, 'moneyFlag', function(bool)
            moneyLoop = bool
            spawn(function()
                while moneyLoop do
                    local args = {"Sell"}
                    game:GetService("ReplicatedStorage"):WaitForChild("House"):FireServer(unpack(args))
                    wait(loopSpeed)
                end
            end)
        end},
        BoxSpeedLoop = {"Speed: ", function(str)
            local val = tonumber(str)
            if val and val > 0 then loopSpeed = val end
        end, tostring(loopSpeed)},
        ButtonAnim = {"อนิเมชั่น SFX", function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/TOUx1/Roblox-script/refs/heads/main/Animation%20sfx?update="..tick()))()
        end},
        ButtonHandcuff = {"หยิบกุญแจมือ", function()
            local CollectModule = loadstring(game:HttpGet("https://raw.githubusercontent.com/wino444/wino444_CollectModule/main/.lua"))()
            _G.AutoCollectModule.CollectItemByName("Handcuff")
        end},
        ToggleAutoEat = {"อ้อโต้กินข้าว", autoEat, 'eatFlag', function(bool)
            autoEat = bool
            if autoEat then
                local CollectModule = loadstring(game:HttpGet("https://raw.githubusercontent.com/wino444/wino444_CollectModule/main/.lua"))()
                _G.AutoCollectModule.CollectItemByName("Tom Yum Kung")
                spawn(function()
                    local hungry = player:WaitForChild("Hungry")
                    local vim = game:GetService("VirtualInputManager")
                    while autoEat do
                        if hungry.Value <= 50 then
                            while hungry.Value < 100 and autoEat do
                                local CollectModule = loadstring(game:HttpGet("https://raw.githubusercontent.com/wino444/wino444_CollectModule/main/.lua"))()
                                _G.AutoCollectModule.CollectItemByName("Tom Yum Kung")
                                wait(0.5)
                                local tool = player.Backpack:FindFirstChild("tom yum kung")
                                if tool then
                                    humanoid:EquipTool(tool)
                                    wait(0.1)
                                    tool = character:FindFirstChild("tom yum kung")
                                    if tool then
                                        while autoEat and hungry.Value < 100 and tool.Parent == character do
                                            vim:SendMouseButtonEvent(0, 0, 0, true, game, 0)
                                            wait(0.05)
                                            vim:SendMouseButtonEvent(0, 0, 0, false, game, 0)
                                            wait(1)
                                        end
                                    end
                                else
                                    wait(1)
                                end
                            end
                        end
                        wait(1)
                    end
                end)
            end
        end},
        ButtonBuyGun = {"ซื้อปืน", function()
            local CollectModule = loadstring(game:HttpGet("https://raw.githubusercontent.com/wino444/wino444_CollectModule/main/.lua"))()
            _G.AutoCollectModule.CollectItemByName("M4")
        end}
    },
    Section4 = {
        LabelAimbot = {"aimbot", 18, 30},
        ButtonAimbot = {"aimbot", function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/wino444/cityThai/refs/heads/main/Aimbot.lua"))()
        end},
        LabelCopy = {"Copy เสื้อ", 18, 30},
        ButtonCopy = {"Copy เสื้อ", function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/wino444/cityThai/refs/heads/main/Copy%20Outfit.lua"))()
        end},
        ButtonPrevent = {"ป้องกันโดนจับ", function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/TOUx1/Roblox-script/refs/heads/main/Touscript"))()
        end},
        ButtonPreventV2 = {"ป้องกันจับV2", function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/TOUxIllIll0O00OO/RobloxscriptTOU/refs/heads/main/Anti%20Got%20caught"))()
        end}
    }
})

local TextChatService = game:GetService("TextChatService")

local function sendMessage(msg)
    local channel = TextChatService:FindFirstChild("TextChannels") and TextChatService.TextChannels:FindFirstChild("RBXGeneral")
    if channel then
        channel:SendAsync(msg)
    else
        warn("❌ ไม่พบแชนเนล RBXGeneral")
    end
end

sendMessage("SCRIPT CITY THAILAND 2 BY:TOUx1")
